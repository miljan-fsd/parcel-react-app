{
  "name": "parcel-react-app",
  "version": "1.0.0",
  "description": "Create React apps using Parcel as build tool",
  "main": "index.js",
  "scripts": {
    "start":
      "cross-env NODE_ENV=development parcel ./public/index.html -p 3000",
    "build":
      "cross-env NODE_ENV=production parcel build ./public/index.html -d build --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miljan-fsd/parcel-react-app.git"
  },
  "keywords": ["Parcel", "React"],
  "author": "Miljan Djordjevic <miljan.fsd@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miljan-fsd/parcel-react-app/issues"
  },
  "homepage": "https://github.com/miljan-fsd/parcel-react-app#readme",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "parcel-bundler": "^1.5.0"
  }
}
